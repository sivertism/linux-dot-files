<externaltool id="Google_Lint">
    <description>Run lint on current CPP file</description>
    <displayname>Google Lint</displayname>
    <category></category>
    <executable output="showinpane" error="showinpane" modifiesdocument="no">
        <path>/usr/local/bin/cpplint.py</path>
        <arguments>%{CurrentDocument:FilePath}</arguments>
    </executable>
</externaltool>
